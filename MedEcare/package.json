    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "vite": "^4.4.5"
