  "version": "0.0.0",
  "type": "module",
  "main": "backend/server.js",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backendServ": "nodemon backend/server.js",
    "dev:backendCapt": "nodemon backend/capteurs.js",
    "backendServ": "NODE_ENV=production node backend/server.js",
    "backendCapt": "NODE_ENV=production node backend/capteurs.js",
    "production": "concurrently \"npm run backendServ\" \"npm run backendCapt\" \"npm run preview\"",
    "dev:development": "cross-env NODE_ENV=development concurrently 'npm:dev:frontend' 'npm:dev:backendServ' 'npm:dev:backendCapt'",
    "build": "vite build",
    "test:component": "cypress run --component",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jose": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
